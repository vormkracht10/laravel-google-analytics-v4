{
    "name": "vormkracht10/laravel-google-analytics",
    "description": "Laravel package to get insights in Google Analytics v4 data",
    "keywords": [
        "vormkracht10",
        "laravel",
        "laravel-google-analytics"
    ],
    "homepage": "https://github.com/vormkracht10/laravel-google-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Bas van Dinther",
            "email": "bas@vormkracht10.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.13.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Vormkracht10\\Analytics\\": "src",
            "Vormkracht10\\Analytics\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vormkracht10\\Analytics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vormkracht10\\Analytics\\AnalyticsServiceProvider"
            ],
            "aliases": {
                "Analytics": "Vormkracht10\\Analytics\\Facades\\Analytics"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
